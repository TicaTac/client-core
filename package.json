{
  "name": "datorama-core",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "npm run test:ci && gulp --gulpfile lib/build-tools/gulpfile.js",
    "commit": "git-cz",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "precommit": "lint-staged",
    "start": "cd playground && npm start",
    "link-bin": "link-parent-bin --child-directory-root .",
    "themes": "cd lib && npm run themify:dev",
    "test:ci": "CI=true karma start lib/config/karma.conf.js",
    "test": "karma start lib/config/karma.conf.js"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
