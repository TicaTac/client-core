{
  "name": "datorama-core",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "gulp --gulpfile lib/build-tools/gulpfile.js",
    "commit": "git-cz",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "precommit": "lint-staged",
    "start": "cd playground && npm start",
    "link-parent-bin": "link-parent-bin --child-directory-root .",
    "themes": "cd lib && npm run watch",
    "postinstall": "link-parent-bin"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^6.1.3",
    "@commitlint/config-angular": "^6.1.3",
    "@commitlint/config-conventional": "^6.1.3",
    "autoprefixer": "^8.1.0",
    "commitizen": "^2.9.6",
    "cz-conventional-changelog": "^2.1.0",
    "del": "^3.0.0",
    "fs": "0.0.1-security",
    "gulp": "^3.9.1",
    "gulp-autoprefixer": "^5.0.0",
    "gulp-clean-css": "^3.9.2",
    "gulp-htmlmin": "^4.0.0",
    "gulp-if": "^2.0.2",
    "gulp-postcss": "^7.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.6.1",
    "gulp-sass": "^3.1.0",
    "gulp-watch": "^5.0.0",
    "hex-to-rgba": "^1.0.1",
    "husky": "^0.14.3",
    "link-parent-bin": "^0.1.3",
    "lint-staged": "^7.0.0",
    "path": "^0.12.7",
    "postcss": "^6.0.19",
    "postcss-node-sass": "^2.1.4",
    "prettier": "^1.11.1",
    "rimraf": "^2.6.2",
    "rollup": "^0.54.0",
    "run-sequence": "^2.2.1",
    "rxjs": "^5.5.6",
    "sass-json-export": "^2.0.0",
    "sorcery": "^0.10.0",
    "sync-json": "^1.0.2",
    "through2": "^2.0.3",
    "zone.js": "^0.8.19"
  },
  "dependencies": {
    "@angular/animations": "^5.2.7",
    "@angular/cli": "1.6.5",
    "@angular/common": "^5.2.7",
    "@angular/compiler": "^5.2.7",
    "@angular/compiler-cli": "^5.2.7",
    "@angular/core": "^5.2.7",
    "@angular/forms": "^5.2.7",
    "@angular/http": "^5.2.7",
    "@angular/language-service": "^5.2.7",
    "@angular/platform-browser": "^5.2.7",
    "@angular/platform-browser-dynamic": "^5.2.7",
    "@angular/router": "^5.2.7",
    "@datorama/utils": "~1.8.0",
    "ag-grid": "^16.0.1",
    "ag-grid-angular": "^16.0.0",
    "ag-grid-enterprise": "^16.0.1",
    "angular-sortablejs": "^2.5.2",
    "helpful-decorators": "^1.7.0",
    "ngx-take-until-destroy": "^2.1.1",
    "popper.js": "^1.12.9",
    "sortablejs": "^1.7.0"
  }
}
